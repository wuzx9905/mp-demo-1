.wrapper{
  display: flex;
  justify-content: space-between;
  flex-direction: column;
  align-items: center;
}

.numbers{
  display: flex;
  justify-content: center;
  align-items: center;
}

.numbers > .number{
  margin:4px;
  padding: 0 10px;
}

.wrapper > .circle{
  margin-bottom: 20px;
  margin-top: 20px;
  border: 1px solid black;
  border-radius: 50%;
  width: 2em;
  height: 2em;
  display: flex;
  justify-content: center;
  align-items: center;
}

.wrapper > .result{
  margin-top: 10px;
}
.wrapper > .beginNum{
  margin-top:  10px;
  margin-bottom: 30px;
}